<html>
<head>
  <meta charset="utf-8">
  <title="register">
</head>
<body>
  <h1>Welcome to Asterism</h1>

  <div class="inp">
  <%= form_tag("/register",method: :post) do %>
    <%= label_tag"名前" %>
    <%= text_field_tag :username%><br>
    <%= label_tag"パスワード" %>
    <%= password_field_tag :password%><br>
    <%=submit_tag"送信" %>
  <% end %>
  </div>

</body>
</html>
